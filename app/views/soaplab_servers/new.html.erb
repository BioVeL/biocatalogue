<h1>New soaplab_server</h1>

<% form_for(@soaplab_server) do |f| %>
  <%= f.error_messages %>
	
	<p>
   	Name <br />
    <%= f.text_field :name %>
  </p>
   <p>
   	Location <br />
    <%= f.text_field :location %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', soaplab_servers_path %>
