<h1>New soap_service</h1>

<% form_for(@soap_service) do |f| %>
  <%= f.error_messages %>
	<p>Name: <br/>
    <%= f.text_field :name %>
  </p>
  
  <p>location: <br/>
    <%= f.text_field :wsdl_location %>
  </p>
  
  <p>Description: <br/>
    <%= f.text_area :description %>
  </p>
  
  <p>
    <%= f.submit "submit new service" %>
  </p>
<% end %>

<%= link_to 'Back', soap_services_path %>
