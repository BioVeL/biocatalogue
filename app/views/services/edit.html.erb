<h1>Editing Service</h1>

<% form_for(@service) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :service_type %><br />
    <%= f.text_field :service_type %>
  </p>
  <p>
    <%= f.label :unique_code %><br />
    <%= f.text_field :unique_code %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @service %> |
<%= link_to 'Back', services_path %>
